# DataTypes

# str 
x = "Hello World" 
# int 
x = 20            	
# float 	
x = 20.5 					
# complex 	
x = 1j 						
# list 	
x = ["apple", "banana", "cherry"] 		
# tuple 	
x = ("apple", "banana", "cherry") 		
# range 	
x = range(6) 					
# dict 	
x = {"name" : "John", "age" : 36} 		
# set 	
x = {"apple", "banana", "cherry"} 		
# frozenset 
x = frozenset({"apple", "banana", "cherry"}) 
# bool
x = True 					

